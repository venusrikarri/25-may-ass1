Class Solution(object):
    def minCostClimbingStairs(self, cost):
        """
        :type cost: List[int]
        :rtype: int
        """
        costLen = len(cost)
        result = [0 for i in range (costLen + 1)]
        for i in range(2, costLen + 1):
            result[i] = min(cost[i - 1] + result[i - 1], cost[i - 2] + result[i - 2])
        return result[costLen]
